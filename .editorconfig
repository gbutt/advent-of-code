# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
indent_style = space
indent_size = 2

[*.py]
indent_style = space
indent_size = 4

[*.{ts,tsx,js,jsx,json,yml}]
indent_style = space
indent_size = 2